{
  "name": "@node-rs/deno-lint",
  "version": "0.2.0",
  "description": "Deno lint binding for NodeJS",
  "keywords": ["Deno", "Lint", "ESLint", "node-rs", "napi", "N-API", "Rust", "napi-rs"],
  "author": "LongYinan <lynweklm@gmail.com>",
  "homepage": "https://github.com/napi-rs/node-rs",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "denolint": "./bin.js"
  },
  "typings": "index.d.ts",
  "files": ["index.js", "index.d.ts", "bin.js", "webpack-loader.js", "LICENSE"],
  "os": ["darwin", "linux", "win32"],
  "cpu": ["x64"],
  "engines": {
    "node": ">= 8.9"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napi-rs/node-rs.git"
  },
  "scripts": {
    "bench": "cross-env NODE_ENV=production node benchmark/lint.js",
    "build": "napi build --platform --release ./deno-lint",
    "build:debug": "napi build --platform ./deno-lint",
    "prepublishOnly": "node ./publish.js",
    "version": "node ./version.js"
  },
  "bugs": {
    "url": "https://github.com/napi-rs/node-rs/issues"
  },
  "dependencies": {
    "@node-rs/helper": "^0.3.0",
    "loader-utils": "^2.0.0"
  },
  "devDependencies": {
    "@types/webpack": "^4.41.21"
  },
  "optionalDependencies": {
    "@node-rs/deno-lint-darwin": "^0.2.0",
    "@node-rs/deno-lint-linux": "^0.1.0",
    "@node-rs/deno-lint-win32": "^0.2.0"
  }
}
